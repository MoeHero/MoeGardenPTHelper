{
  "name": "moegarden-pt-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:index && concurrently -n Background,Content \"npm run build:background\" \"npm run build:content\"",
    "build:index": "vue-cli-service build --modern",
    "build:background": "webpack --config webpack/prod.background.config.js --progress",
    "build:content": "webpack --config webpack/prod.content.config.js --progress",
    "dev": "npm run dev:index && concurrently -n Background,Content \"npm run dev:background\" \"npm run dev:content\"",
    "dev:index": "vue-cli-service build --modern --mode=development",
    "dev:background": "webpack --config webpack/dev.background.config.js --progress",
    "dev:content": "webpack --config webpack/dev.content.config.js --progress",
    "watch": "concurrently -n Index,Background,Content \"npm run watch:index\" \"npm run watch:background\" \"npm run watch:content\"",
    "watch:index": "vue-cli-service build --modern --watch --mode=development",
    "watch:background": "webpack --config webpack/dev.background.config.js --progress --watch",
    "watch:content": "webpack --config webpack/dev.content.config.js --progress --watch",
    "commit": "./node_modules/.bin/git-cz",
    "release": "./node_modules/.bin/standard-version",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^6.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "standard-version": "^9.3.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-vuetify": "^2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
